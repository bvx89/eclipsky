<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="no.hal.eclipsky.services.workspace.http.WorkspaceHttpServiceImpl">
	<implementation
		class="no.hal.eclipsky.services.workspace.http.WorkspaceHttpServiceImpl"
	/>
   	<reference
   		name="httpService"
   		interface="org.osgi.service.http.HttpService"
   		bind="setHttpService"
   	/>
   	<reference
   		name="serviceServlet"
   		interface="no.hal.eclipsky.services.workspace.http.ServiceServlet"
   		cardinality="0..n"
   		policy="dynamic"
   		bind="addServiceServlet"
   		unbind="removeServiceServlet"
   	/>
</scr:component>
